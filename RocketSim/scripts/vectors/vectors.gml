
function vec2(v1,v2){
	var va = ds_list_create();
	va[0]=v1;
	va[1]=v2;
	return va;
}

function vec3(v1,v2,v3){
	var va = ds_list_create();
	va[0]=v1;
	va[1]=v2;
	va[2]=v3;
	return va;
}

function vec4(v1,v2,v3,v4){
	var va = ds_list_create();
	va[0]=v1;
	va[1]=v2;
	va[2]=v3;
	va[3]=v4;
	return va;
}
